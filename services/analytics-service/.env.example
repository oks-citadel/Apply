# Application
NODE_ENV=development
PORT=8006

# API Configuration
API_BASE_URL=http://localhost:8006
FRONTEND_URL=http://localhost:3000
CORS_ORIGINS=*

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=jobpilot_analytics
DB_SYNCHRONIZE=true
DB_LOGGING=true

# Analytics Configuration
ANALYTICS_RETENTION_DAYS=90
ANALYTICS_AGGREGATION_INTERVAL=3600000
ANALYTICS_ENABLE_REALTIME=true
ANALYTICS_MAX_EVENTS_BATCH=1000

# Export Configuration
EXPORT_MAX_RECORDS=50000
EXPORT_FORMATS=csv,json
EXPORT_CHUNK_SIZE=5000

# Rate Limiting
THROTTLE_TTL=60000
THROTTLE_LIMIT=100

# Cache Configuration
CACHE_TTL=300
CACHE_MAX=1000
