{
  "dashboard": {
    "title": "ApplyForUs - Business Metrics",
    "tags": ["applyforus", "business", "kpi"],
    "timezone": "browser",
    "refresh": "1m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Job Applications - Total (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(job_applications_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 2,
        "title": "Job Applications Rate (per hour)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(job_applications_total[1h])) * 3600",
            "legendFormat": "Applications/hour",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Applications/hour"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Application Success Rate (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(job_applications_total{status=\"success\"}[5m])) / sum(rate(job_applications_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success %", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Resume Generations (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(resume_generation_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "title": "AI Service Requests",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(ai_requests_total[5m])) by (provider, model)",
            "legendFormat": "{{ provider }} - {{ model }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "AI Service Success Rate (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(ai_requests_total{status=\"success\"}[5m])) / sum(rate(ai_requests_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success %"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "AI Request Duration - P95 (seconds)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ai_request_duration_seconds_bucket[5m])) by (le, provider))",
            "legendFormat": "{{ provider }} P95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Active Users",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "user_active_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 9,
        "title": "Payment Transactions (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(payment_transactions_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 10,
        "title": "Payment Success Rate (%)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(payment_transactions_total{status=\"success\"}[5m])) / sum(rate(payment_transactions_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success %"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Queue Health - Waiting Jobs",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "bull_queue_waiting",
            "legendFormat": "{{ queue }}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Jobs Waiting"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Queue Processing Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "sum(rate(bull_queue_completed_total[5m])) by (queue)",
            "legendFormat": "{{ queue }} - Completed",
            "refId": "A"
          },
          {
            "expr": "sum(rate(bull_queue_failed_total[5m])) by (queue)",
            "legendFormat": "{{ queue }} - Failed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Jobs/sec"},
          {"format": "short"}
        ]
      }
    ]
  }
}
